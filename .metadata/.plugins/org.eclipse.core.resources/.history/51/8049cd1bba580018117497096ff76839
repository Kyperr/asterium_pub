package com.toozo.asteriumwebserver.gamelogic.items;

public abstract class Item {
	public static final String DEFAULT_NAME = "";
	private String name;
	
	// ===== CONSTRUCTORS =====
	protected Item(String name) {
		this.name = name;
	}
	// ========================
	
	// ===== GETTERS =====
	public String getName() {
		if (this.name != null) {
			return this.name;
		} else {
			return DEFAULT_NAME;
		}
	}
	// ===================
}
