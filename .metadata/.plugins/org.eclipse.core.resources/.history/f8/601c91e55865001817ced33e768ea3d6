	public LowerHealth(PlayerCharacter owner, Map<Stat, Function<Integer, Integer>> statModifiers) {
		super(owner, statModifiers);
		// TODO Auto-generated constructor stub
	}

	public LowerHealth(PlayerCharacter owner, String name, Map<Stat, Function<Integer, Integer>> statModifiers) {
		super(owner, name, statModifiers);
		// TODO Auto-generated constructor stub
	}

	public LowerHealth(PlayerCharacter owner, String name, int duration,
			Map<Stat, Function<Integer, Integer>> statModifiers) {
		super(owner, name, duration, statModifiers);
		// TODO Auto-generated constructor stub
	}

}
